<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>kestrel</string>

    <key>ProgramArguments</key>
    <array>
        <string>java</string>
        <string>-server</string>
        <string>-Xmx1024m</string>
        <string>-Dstage=development</string>
        <string>-jar</string>
        <string>/usr/local/Cellar/kestrel/2.4.1/libexec/kestrel_2.9.2-2.4.1.jar</string>
    </array>
    
    <key>KeepAlive</key>
    <true/>

    <key>OnDemand</key>
    <false/>

    <key>Nice</key>
    <integer>10</integer>

    <key>StartInterval</key>
    <integer>60</integer>

    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/kestrel/stderr</string>

    <key>StandardOutPath</key>
    <string>usr/local/var/log/kestrel/stdout</string>
</dict>
</plist>
